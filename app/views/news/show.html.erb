<% if signed_in? %>
 <p><%= link_to "編集", edit_news_path(@news.id) %></p>
 <p><%= link_to "削除", news_path(@news.id), method: :delete %></p>
 <% end %>
<div class="news-main">
<p class="nn-normal"><%= @news.news_title %></p>
<p class="nn-created-at"><%= @news.created_at.strftime("%Y-%m-%d") %></p>
<p class="n-normal"><%= @news.latest_news %></p>
</div>
